{
    "name": "create-fast-context",
    "version": "0.5.0",
    "main": "dist/index.js",
    "source": "./src/index.ts",
    "types": "dist/index.d.ts",
    "repository": "https://github.com/andrew-colman/create-fast-context",
    "author": {
        "name": "Andrew Colman"
    },
    "keywords": [
        "create",
        "fast",
        "context",
        "react",
        "typescript"
    ],
    "license": "MIT",
    "scripts": {
        "prepublishOnly": "npm run build",
        "build": "rollup --config rollup.config.js",
        "build:quick": "sucrase ./src -d ./dist --transforms typescript,imports",
        "minify": "terser dist/index.js --compress ecma=2015,computed_props=false -o dist/index.js --comments false",
        "dev": "nodemon ./src/index.ts ",
        "predev:compiled": "yarn build:quick",
        "dev:compiled": "node dist/index.js",
        "test": "jest -i",
        "test:verbose": "jest --verbose",
        "test:watch": "jest --watch",
        "prepare": "husky install",
        "lint": "eslint --cache --fix",
        "format": "prettier --write",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.21.5",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.21.5",
        "@rollup/plugin-commonjs": "^22.0.2",
        "@rollup/plugin-node-resolve": "^14.1.0",
        "@rollup/plugin-typescript": "^11.0.0",
        "@storybook/addon-essentials": "7.0.7",
        "@storybook/addon-interactions": "7.0.7",
        "@storybook/addon-links": "7.0.7",
        "@storybook/blocks": "7.0.7",
        "@storybook/react": "7.0.7",
        "@storybook/react-webpack5": "7.0.7",
        "@storybook/testing-library": "0.0.14-next.2",
        "@swc/core": "^1.3.1",
        "@swc/jest": "^0.2.22",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^29.0.3",
        "@types/node": "^18.7.18",
        "@types/react": "^18.0.25",
        "@typescript-eslint/eslint-plugin": "^5.37.0",
        "@typescript-eslint/parser": "^5.37.0",
        "eslint": "^8.23.1",
        "eslint-config-prettier": "^8.4.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.6.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.0.1",
        "husky": "^8.0.1",
        "jest": "^29.0.3",
        "jest-environment-jsdom": "^29.4.0",
        "jsonc-require": "^1.0.1",
        "lint-staged": "^13.0.3",
        "nodemon": "^2.0.20",
        "prettier": "^2.7.1",
        "prop-types": "15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rollup": "^2.79.0",
        "rollup-plugin-command": "^1.1.3",
        "rollup-plugin-delete": "^2.0.0",
        "rollup-plugin-dts": "^4.2.2",
        "rollup-plugin-preserve-shebang": "^1.0.1",
        "storybook": "7.0.7",
        "sucrase": "^3.27.0",
        "terser": "^5.15.0",
        "ts-jest": "^29.0.1",
        "tslib": "^2.4.0",
        "typescript": "^4.8.3"
    },
    "dependencies": {},
    "lint-staged": {
        "*.{js,ts}": "npm run lint",
        "*.{js,ts,css,md}": "npm run format"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    }
}
